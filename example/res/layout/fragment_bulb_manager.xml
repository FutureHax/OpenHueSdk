<com.t3hh4xx0r.hueopensdkexample.DashboardLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".BulbManagerActivity$DummySectionFragment" >

    <Button
        android:id="@+id/rename"
        style="@style/DashboardButton"
        android:text="Rename" />

   <Button
        android:id="@+id/on"
        style="@style/DashboardButton"
        android:text="On" />    

    <Button
        android:id="@+id/off"
        style="@style/DashboardButton"
        android:text="Off" />

    <Button
        android:id="@+id/color_pickers"
        style="@style/DashboardButton"
        android:text="Color pickers" />

    <Button
        android:id="@+id/alerts"
        style="@style/DashboardButton"
        android:text="Alert" />
</com.t3hh4xx0r.hueopensdkexample.DashboardLayout>